	'comment': {
		pattern: /\/\/.*|\/\*(?:[^*/]|\*(?!\/)|\/(?!\*)|\/\*(?:[^*]|\*(?!\/))*\*\/)*\*\//,
		greedy: true
	},
	'string': {
		pattern: /(^|[^\\])"(?:[^"\\]|\\[\s\S])*"/,
		lookbehind: true,
		greedy: true
	},

	var arrowRight = /(?:>{1,2}|\/{1,2}|\\{1,2}|\|>|[#*^+{xo])/.source;
	var arrowPrefix = /[[?]?[ox]?/.source;
	var arrowSuffix = /[ox]?[\]?]?/.source;
	var arrow =
		arrowPrefix +
		'(?:' +
		arrowBody + arrowRight +
		'|' +
		arrowLeft + arrowBody + '(?:' + arrowRight + ')?' +
		')' +
		arrowSuffix;


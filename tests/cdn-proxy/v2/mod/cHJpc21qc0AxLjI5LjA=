	'function': /[a-zA-Z–∞-—è–ê-–Ø_\u00C0-\u00D6\u00D8-\u00F6\u00F8-\u00FF$][\w–∞-—è–ê-–Ø\u00C0-\u00D6\u00D8-\u00F6\u00F8-\u00FF$]*\s*(?=\()/,
	'number': /\b\d+(?:\.\d+)?\b|\B\.\d+\b/,
	'operator': /[<>!]=?|-|\+|&&|==|\|\|?|\/\/?|[?:*^%]/,
	'boolean': /\b(?:false|true)\b/,
	'keyword': /\bin\b/,
	'punctuation': /[{}[\](),.]/,
};

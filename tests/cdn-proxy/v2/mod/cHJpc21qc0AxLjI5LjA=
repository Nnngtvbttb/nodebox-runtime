	var variable = /\$\w+|%[a-z]+%/;

	var arrowAttr = /\[[^[\]]*\]/.source;
	var arrowDirection = /(?:[drlu]|do|down|le|left|ri|right|up)/.source;
	var arrowBody = '(?:-+' + arrowDirection + '-+|\\.+' + arrowDirection + '\\.+|-+(?:' + arrowAttr + '-*)?|' + arrowAttr + '-+|\\.+(?:' + arrowAttr + '\\.*)?|' + arrowAttr + '\\.+)';
